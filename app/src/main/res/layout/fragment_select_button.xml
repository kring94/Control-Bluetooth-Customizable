<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.settings.SelectButtonFragment">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginTop="@dimen/padding_large"
        android:src="@drawable/uno"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_select_text" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginEnd="@dimen/padding_large"
        android:src="@drawable/dos"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginStart="@dimen/padding_large"
        android:src="@drawable/cuatro"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginTop="@dimen/padding_large"
        android:src="@drawable/tres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginTop="@dimen/padding_large"
        android:src="@drawable/cinco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginEnd="@dimen/padding_large"
        android:src="@drawable/seis"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginStart="@dimen/padding_large"
        android:src="@drawable/siete"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginEnd="@dimen/padding_large"
        android:src="@drawable/ocho"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="@dimen/buttons_dimens"
        android:layout_height="@dimen/buttons_dimens"
        android:layout_marginStart="@dimen/padding_large"
        android:src="@drawable/nueve"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <TextView
        android:id="@+id/title_select_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_large"
        android:fontFamily="@font/roboto"
        android:textSize="@dimen/title_dimens"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Select your new Button" />
</androidx.constraintlayout.widget.ConstraintLayout>